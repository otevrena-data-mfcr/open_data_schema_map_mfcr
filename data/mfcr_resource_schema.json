{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://project-open-data.github.io/schema/1_0_final/single_entry.json#",
  "title": "CKAN Resource Schema",
  "description": "The metadata format for all federal open data. Validates a single JSON object entry (as opposed to entire Data.json catalog).",
  "type": "object",
  "properties": {
    "id": {
      "title": "ID",
      "type": "string"
    },
    "title": {
      "title": "Název distribuce datové sady",
      "type": "string"
    },
    "license_autorske_dilo": {
      "title": "Specifikace podmínek užití - Autorské dílo",
      "type": "string"
    },
    "license_originalni_databaze": {
      "title": "Specifikace podmínek užití - Originální databáze",
      "type": "string"
    },
    "license_zvlastni_prava_databaze": {
      "title": "Specifikace podmínek užití - Ochrana zvláštními právy pořizovatele databáze",
      "type": "string"
    },
    "license_osobni_udaje": {
      "title": "Specifikace podmínek užití - Osobní údaje",
      "type": "string"
    },
    "downloadURL": {
      "title": "Odkaz na stažení souboru",
      "type": "string"
    },
    "accessURL": {
      "title": "Přístupové URL",
      "type": "string"
    },
    "format": {
      "title": "Formát souboru ke stažení",
      "type": "string"
    },
    "mimetype": {
      "title": "Media type souboru ke stažení",
      "type": "string"
    },
    "conformsTo": {
      "title": "Odkaz na strojově čitelné schéma souboru ke stažení",
      "type": "string"
    },
    "compressFormat": {
      "title": "Media type kompresního formátu",
      "type": "string"
    },
    "packageFormat": {
      "title": "Media type balíčkovacího formátu",
      "type": "string"
     },
    "sch_ma": {
      "title": "Schéma",
      "type": "string"
    },
    "service_endpointURL": {
      "title": "Datová služba - Přístupový bod",
      "type": "string"
    },
    "service_endpointDescription": {
      "title": "Datová služba - Popis přístupového bodu",
      "type": "string"
    }
  }
}
